{
  "name": "addok-server",
  "version": "0.0.0",
  "description": "Full-featured HTTP API for addok",
  "main": "index.js",
  "type": "module",
  "repository": "https://github.com/livingdata-co/addok-server",
  "author": "Jérôme Desboeufs <jerome@livingdata.co>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "xo"
  },
  "devDependencies": {
    "xo": "^0.53.1"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/prevent-abbreviations": "off"
    }
  },
  "engines": {
    "node": ">= 18.12"
  }
}
