{
  "name": "addok-server",
  "version": "0.0.0",
  "description": "Full-featured HTTP API for addok",
  "main": "index.js",
  "type": "module",
  "repository": "https://github.com/livingdata-co/addok-server",
  "author": "Jérôme Desboeufs <jerome@livingdata.co>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "xo",
    "start": "node server"
  },
  "dependencies": {
    "@livingdata/tabular-data-helpers": "^0.0.6",
    "addok-cluster": "^0.3.2",
    "addok-geocode-stream": "^0.21.0",
    "content-disposition": "^0.5.4",
    "cors": "^2.8.5",
    "csv-write-stream": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "into-stream": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "xo": "^0.53.1"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "camelcase": "off",
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/prevent-abbreviations": "off",
      "import/no-unassigned-import": "off"
    }
  },
  "engines": {
    "node": ">= 18.12"
  }
}
